<div id="map"></div>

<div id="floating-panel">

    <b>Start: </b>
    <select id="start">
      
      <option value="40.3932613, -3.6991178">IronHack</option>
      
    </select>
    <b>End: </b>

    <select id="end">
		{{#each tuppers}}
		<option value={{this.user.location.coordinates}}>{{this.user.username}}</option>
		{{/each}}	
    </select>
    {{#each tuppers}}
    <ul>
     
      <li>
        <p>{{this.user.username}}</p>
        <p>{{this.tuppername}}</p>
        <p>{{this.price}}</p>
         {{#if this.user.isCooker}}
        <p><a id="deleteTupper" href="/tupper/delete/{{this._id}}">Delete</a></p>
         {{/if}}
      </li>
      
    </ul>
     {{/each}}
<script>
  window.tuppers = {{{ tupperStr }}}
</script>

<script src="/javascripts/listMap.js"></script>